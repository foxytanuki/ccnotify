# npm configuration for ccnotify

# Ensure npm doesn't modify package-lock.json when we're using pnpm
package-lock=false

# Automatically answer "yes" to any prompts npm might present
yes=true

# Set registry (default)
registry=https://registry.npmjs.org/

# Save exact versions in package.json
save-exact=false

# Don't save prefix '^' for versions
save-prefix=''

# Access level for publishing
access=public

# Engine strict mode
engine-strict=true